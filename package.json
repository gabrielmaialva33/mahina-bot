{
  "name": "mahina-bot",
  "version": "3.0.1",
  "description": "MahinaBot is a music bot for Discord, written in Typescript, using Lavalink as the audio player.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmaialva33/mahina-bot.git"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "lavalink-client",
    "mahinabot",
    "typescript",
    "prisma"
  ],
  "author": "gabrielmaialva33",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielmaialva33/mahina-bot/issues"
  },
  "homepage": "https://github.com/gabrielmaialva33/mahina-bot#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf build && tsup",
    "start": "node dist/index.js",
    "start:max": "node --max-old-space-size=4096 --optimize_for_size dist/index.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev --name init"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#common/*": "./src/common/*.js",
    "#commands/*": "./src/commands/*.js",
    "#utils/*": "./src/utils/*.js"
  },
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.11.0"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "bundle": true,
    "outDir": "dist"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/i18n": "^0.13.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/signale": "^1.4.7",
    "esbuild-copy-files": "^0.2.16",
    "eslint": "^8.57.1",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.15.1",
    "@gabrielmaialva33/discord-video-stream": "^2.0.5",
    "@prisma/client": "^5.22.0",
    "@top-gg/sdk": "^3.1.6",
    "axios": "^1.7.7",
    "discord.js": "^14.16.3",
    "discord.js-selfbot-v13": "^3.4.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fluent-ffmpeg": "^2.1.3",
    "genius-lyrics-api": "^3.2.1",
    "i18n": "^0.15.1",
    "lavalink-client": "github:tomato6966/lavalink-client",
    "module-alias": "^2.2.3",
    "multer": "1.4.5-lts.1",
    "node-system-stats": "^1.3.0",
    "p-cancelable": "^4.0.1",
    "progress-estimator": "^0.3.1",
    "rimraf": "^6.0.1",
    "signale": "^1.4.0",
    "topgg-autoposter": "^2.0.2",
    "undici": "^6.21.0",
    "youtube-dl-exec": "^3.0.10",
    "zod": "^3.23.8"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}

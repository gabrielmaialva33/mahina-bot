{
  "name": "mahina-bot",
  "version": "3.0.0",
  "description": "MahinaBot is a music bot for Discord, written in JavaScript using the Discord.js, Typescript, lavalink-client (Lavalink) library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmaialva33/mahina-bot.git"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "lavalink-client",
    "mahinabot",
    "typescript",
    "prisma"
  ],
  "author": "gabrielmaialva33",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielmaialva33/mahina-bot/issues"
  },
  "homepage": "https://github.com/gabrielmaialva33/mahina-bot#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf build && tsup",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev --name init"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#common/*": "./src/common/*.js",
    "#commands/*": "./src/commands/*.js",
    "#utils/*": "./src/utils/*.js"
  },
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.11.0"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "bundle": true,
    "outDir": "dist"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@types/i18n": "^0.13.12",
    "@types/node": "^22.8.7",
    "@types/signale": "^1.4.7",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@top-gg/sdk": "^3.1.6",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "genius-lyrics-api": "^3.2.1",
    "i18n": "^0.15.1",
    "lavalink-client": "github:tomato6966/lavalink-client",
    "module-alias": "^2.2.3",
    "node-system-stats": "^1.3.0",
    "rimraf": "^6.0.1",
    "signale": "^1.4.0",
    "topgg-autoposter": "^2.0.2",
    "undici": "^6.20.1",
    "zod": "^3.23.8"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}

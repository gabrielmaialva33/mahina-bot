{
  "name": "winx-discord-bot",
  "version": "1.0.0",
  "description": "A simple discord bot to play videos on voice channels",
  "keywords": [
    "typescript",
    "template"
  ],
  "author": {
    "name": "Gabriel Maia",
    "url": "https://github.com/gabrielmaialva33"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielmaialva33/winx-discord-bot"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf build && tsup",
    "start": "node build/main.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "bundle": true,
    "outDir": "build"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@types/node": "^20.11.25",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@dank074/discord-video-stream": "^3.0.5",
    "@swc/core": "^1.4.5",
    "discord.js-selfbot-v13": "^3.1.4",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "opus": "^0.0.0",
    "opusscript": "^0.1.1",
    "reflect-metadata": "^0.2.1",
    "source-map-support": "^0.5.21"
  }
}
